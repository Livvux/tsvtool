{
  "name": "tsvtool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=6144' next dev --turbo",
    "dev:webpack": "NODE_OPTIONS='--max-old-space-size=6144' next dev",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' next build",
    "build:analyze": "ANALYZE=true NODE_OPTIONS='--max-old-space-size=8192' next build",
    "build:production": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=8192' next build",
    "start": "next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "clean:cache": "bash scripts/clean-cache.sh",
    "clean:install": "rm -rf node_modules && pnpm install",
    "seed:users": "npx tsx scripts/create-seed-users.ts",
    "sync:env": "npx tsx scripts/sync-env-to-convex.ts",
    "sync:env:dry": "npx tsx scripts/sync-env-to-convex.ts --dry-run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.943.0",
    "@aws-sdk/s3-request-presigner": "^3.943.0",
    "@clerk/nextjs": "^6.35.6",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.30.0",
    "date-fns": "^4.1.0",
    "isomorphic-dompurify": "^2.33.0",
    "lucide-react": "^0.555.0",
    "next": "^16.0.7",
    "next-themes": "^0.4.6",
    "react": "^19.2.1",
    "react-day-picker": "^9.11.3",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.68.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@next/bundle-analyzer": "^16.0.7",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "baseline-browser-mapping": "^2.9.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.7",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.2.0",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.15"
  }
}
